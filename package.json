    "@angular/platform-browser-dynamic": "^19.0.0",
    "@angular/router": "^19.0.0",
    "@swimlane/ngx-charts": "^23.0.0-alpha.0",
    "chart.js": "^4.5.0",
    "chromedriver": "^138.0.3",
    "ng2-charts": "^8.0.0",
